library(ggplot2)
library(ggrepel)
setwd("~/desktop/info201/ski-blade/")
ski.resorts <- read.csv("./coordinates.csv", stringsAsFactors = FALSE, header = TRUE)
wa <- get_googlemap(center = "wenatchee wa", zoom = 6, size = c(400, 200), maptype = "roadmap")
ggmap(wa) +
geom_point(data = ski.resorts, aes(x = lon, y = lat), fill = "blue", alpha = .7, size = 5, shape = 21) +
geom_label_repel(data = ski.resorts, aes(x = lon, y = lat, label = name), nudge_x = .7, nudge_y = .2) +
guides(fill=FALSE, alpha=FALSE, size=FALSE)  + ggtitle("Ski Resorts in Washington:")
